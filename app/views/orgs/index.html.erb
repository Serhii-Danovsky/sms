<p id="notice"><%= notice %></p>

<h1>Listing Orgs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Erdpou</th>
      <th>City</th>
      <th>Address</th>
      <th>Osoby</th>
      <th>Creator</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @orgs.each do |org| %>
      <tr>
        <td><%= org.name %></td>
        <td><%= org.erdpou %></td>
        <td><%= org.city %></td>
        <td><%= org.address %></td>
        <td><%= org.osoby %></td>
        <td><%= org.creator %></td>
        <td><%= org.phone %></td>
        <td><%= link_to 'Show', org %></td>
        <td><%= link_to 'Edit', edit_org_path(org) %></td>
        <td><%= link_to 'Destroy', org, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Org', new_org_path %>
